{
  "name": "pleseai-eslint-monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.2",
  "description": "The AI-ready formatter that helps you write and generate code faster.",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "test:coverage": "turbo test:coverage",
    "check": "eslint .",
    "fix": "eslint . --fix",
    "bump-deps": "npx npm-check-updates --deep -u -x recharts && bun install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
