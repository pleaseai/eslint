{
  "name": "@pleaseai/lint",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "bun@1.3.2",
  "description": "Convert ESLint config to AI rules for coding assistants",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js"
  },
